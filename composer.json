{
    "name": "codefog/contao-cookiebar",
    "description": "cookiebar extension for Contao Open Source CMS",
    "keywords": ["contao", "cookie", "cookielaw", "cookiebar"],
    "type": "contao-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.9"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-0": {
            "Codefog\\Cookiebar\\": "src/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/cookiebar"
            }
        }
    }
}
