{
    "name": "codefog/contao-cookiebar",
    "description": "cookiebar extension for Contao Open Source CMS",
    "keywords": ["contao", "cookie", "cookielaw"],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "http://codefog.pl"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core": ">=3.2",
        "contao-community-alliance/composer-plugin": "2.*"
    },
    "replace": {
        "contao-legacy/cookiebar": "self.version"
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/cookiebar"
            }
        }
    }
}
